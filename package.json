{
  "name": "rouster",
  "version": "0.0.14",
  "description": "A basic wrapper around docker for node",
  "main": "index.js",
  "bin": {
    "rouster": "./bin/rouster"
  },
  "scripts": {
    "test": "lab --transform ./test/setup.js --ignore 'Reflect,core,_babelPolyfill,regeneratorRuntime,__core-js_shared__'",
    "compile": "rm -rf dist/; babel -d dist/ lib/",
    "kill": "docker kill $(docker ps -q)",
    "rm": "docker rm -v $(docker ps -qa)",
    "preuninstall": "rm -rf dist/",
    "install": "npm run compile"
  },
  "keywords": [
    "Docker"
  ],
  "author": "Jeremy Darling <jeremy.darling@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.2",
    "babel-cli": "^6.6.x",
    "babel-core": "^6.7.x",
    "babel-preset-es2015": "^6.6.x",
    "babel-preset-stage-2": "^6.5.x",
    "babel-register": "^6.7.x"
  },
  "devDependencies": {
    "code": "^2.1.0",
    "lab": "^10.3.0",
    "nodemon": "^1.8.1"
  }
}
